{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "strict": true,
        "jsx": "preserve",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "allowJs": true,
        "noEmitOnError": true,
        "sourceMap": true
    },
    "include": [
        "src/**/*.ts"
    ]
}